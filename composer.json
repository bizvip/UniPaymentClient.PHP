{
  "name": "unipayment/client",
  "description": "This SDK provides a convenient abstraction of UniPayment's Gateway API and allows developers to focus on payment flow/e-commerce integration rather than on the specific details of client-server interaction using the raw API.",
  "license": "MIT",
  "version": "1.1.0",

  "keywords": [
    "unipayment",
    "bitcoin",
    "eth",
    "usdt",
    "usdc",
    "payment",
    "gateway"
  ],
  "homepage": "https://unipayment.io",
  "authors": [
    {
      "name": "vincent",
      "homepage": "https://unipayment.io"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1"
  },
  "autoload": {
    "psr-4": {
      "UniPayment\\Client\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UniPayment\\Client\\": "test/"
    }
  }
}



